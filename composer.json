{
  "name": "brucetruth/ml-idea",
  "homepage": "https://github.com/brucetruth/ml-idea",
  "description": "A production-ready machine learning library for PHP",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "ai",
    "machine-learning",
    "php",
    "classification"
  ],
  "autoload": {
    "psr-4": {
      "ML\\IDEA\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ML\\IDEA\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.2",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5",
    "phpstan/phpstan": "^1.12"
  },
  "scripts": {
    "test": "phpunit --configuration phpunit.xml.dist",
    "analyse": "phpstan analyse --configuration phpstan.neon"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
